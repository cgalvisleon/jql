{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "server",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/server",
      "args": ["-port", "3500", "-rpc_port", "4500"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env"
    }
  ],
  "compounds": [
    {
      "name": "server",
      "configurations": ["server"],
      "stopAll": true
    }
  ]
}
